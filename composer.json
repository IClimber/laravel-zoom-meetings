{
    "name": "iclimber/laravel-zoom-meetings",
    "description": "Create Zoom Meetings",
    "keywords": [
        "philharmonie",
        "iclimber",
        "laravel",
        "laravel-zoom-meetings"
    ],
    "homepage": "https://github.com/IClimber/laravel-zoom-meetings",
    "license": "MIT",
    "authors": [
        {
            "name": "IClimber",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0|^8.1|^8.2",
        "guzzlehttp/guzzle": "^7.5",
        "illuminate/contracts": "^8.0|^9.0|^10.0",
        "spatie/laravel-package-tools": "^1.12.1",
        "ext-json": "*"
    },
    "require-dev": {
        "nunomaduro/collision": "^v5.11.0",
        "orchestra/testbench": "^v6.27.1"
    },
    "autoload": {
        "psr-4": {
            "IClimber\\LaravelZoomMeetings\\": "src/"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "IClimber\\LaravelZoomMeetings\\LaravelZoomMeetingsServiceProvider"
            ],
            "aliases": {
                "LaravelZoomMeetings": "IClimber\\LaravelZoomMeetings\\Facades\\LaravelZoomMeetings"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
